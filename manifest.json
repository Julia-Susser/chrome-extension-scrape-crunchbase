{
    "manifest_version": 3,
    "name": "Hello World Extension",
    "version": "1.0",
    "permissions": ["storage", "scripting","fileSystem", "activeTab", "tabs","http://*/*", "https://*/*"],
    "background": {
        "service_worker": "background.js",
        "persistent": true
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
